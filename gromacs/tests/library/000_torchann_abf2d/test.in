  extendedFluctuation 5.0
  extendedTimeConstant 500
  extendedTemp 300

  torchann {
    model_file dihedrals_cpu.pt
    m_output_index 1
    period 6.2832
    atoms {
      atomNumbers {
	2 5 6 7 9 11 15 16 17 19
      }
    }
  }
}


abf {
  colvars nn_phi nn_psi
  fullSamples	2000
}

