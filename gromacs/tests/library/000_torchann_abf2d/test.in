colvarsTrajFrequency 1
colvarsRestartFrequency 10

colvar {
  name nn_phi
  lowerBoundary -3.3
  upperBoundary 3.3
  width 0.1
  extendedLagrangian on
  extendedTemp 300
  extendedFluctuation 5.0
  extendedTimeConstant 500

  torchann {
    model_file dihedrals_cpu.pt
    m_output_index 0
    period 6.2832
    atoms {
      atomNumbers {
	2 5 6 7 9 11 15 16 17 19
      }
    }
  }
}

colvar {
  name nn_psi
  lowerBoundary -3.3
  upperBoundary 3.3
  width 0.1
